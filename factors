#!/usr/bin/python3

import sys
import time

def factorize(num):
    """
    Takes number as input
    Args: 
        num: input integer
    Return: 
        Tuple with two factors if not prime
        None if prime
    """
    for i in range(2, int(num**0.5)+1):
        if num % i == 0:
            return (i, num // i)
    return None

def process_file(input_file):
    try:
        with open(input_file, 'r') as f:
            return f.readlines()
    except FileNotFoundError:
        print("File not found")
        return []

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        sys.exit(1)

    input_file = sys.argv[1]
    lines = process_file(input_file)
    
    if not lines:
        sys.exit(1)

    start_time = time.time()

    for line in lines:
        try:
            num = int(line.strip())
            factors = factorize(num)

            if factors:
                # Sort factors before printing
                sorted_factors = sorted(factors)
                print(f"{num}={sorted_factors[0]}*{sorted_factors[1]}")

            if time.time() - start_time > 5:
                print("Time limit exceeded")
                break
        except ValueError:
            print("Invalid number on input file")

if __name__ == '__main__':
    main()
